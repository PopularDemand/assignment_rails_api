<h1>Smelly Potatoes Movies</h1>

<h2>Create a Movie</h2>

<%= form_for Movie.new do |f| %>
    Title: 
    <%= f.text_field :title %>
    <%= f.submit %>
<% end %>

<h2>All Movies</h2>

<table id='all-movies'>
  <thead>
    <tr>
      <th>Title</th>
      <th>Release Date</th>
    </tr>
  </thead>
  <tbody id='movies-list'>
  </tbody>
</table>